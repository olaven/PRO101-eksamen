#!/bin/sh
DYLD_FALLBACK_LIBRARY_PATH=/Applications/wordpress-4.9.4-1/mysql/lib:/Applications/wordpress-4.9.4-1/mysql/../common/lib:$DYLD_FALLBACK_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH
case "$@" in
  *--no-defaults*)
    exec $0.bin "$@"
    ;;
  *--defaults-extra-file*)
    exec $0.bin "$@"
    ;;
esac
exec $0.bin --defaults-file=/Applications/wordpress-4.9.4-1/mysql/my.cnf "$@"
