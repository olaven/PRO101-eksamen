#!/bin/sh
LD_LIBRARY_PATH="/Applications/wordpress-4.9.4-1/common/lib:$LD_LIBRARY_PATH"
DYLD_FALLBACK_LIBRARY_PATH="/Applications/wordpress-4.9.4-1/common/lib:$DYLD_FALLBACK_LIBRARY_PATH"
OPENSSL_CONF="/Applications/wordpress-4.9.4-1/common/openssl/openssl.cnf"
OPENSSL_ENGINES="/Applications/wordpress-4.9.4-1/common/lib/engines"
export LD_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH
export OPENSSL_CONF
export OPENSSL_ENGINES
exec /Applications/wordpress-4.9.4-1/common/bin/openssl.bin "$@"
